{
  "name": "zc2-chinese-service",
  "version": "0.5.5",
  "description": "Chinese astrology microservice for ZodiaCore - handles Chinese zodiac calculations, predictions, and consultations",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "watch": "nodemon server.js",
    "build": "echo 'Chinese service build not required for Node.js'",
    "build:prod": "echo 'Chinese service build not required for Node.js'",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "docker:build": "docker build -t zc2-chinese-service .",
    "docker:run": "docker run -p 4002:4002 zc2-chinese-service",
    "deploy": "render deploy",
    "deploy:staging": "render deploy --service zc2-chinese-staging",
    "deploy:prod": "render deploy --service zc2-chinese-prod"
  },
  "keywords": [
    "astrology",
    "chinese",
    "zodiac",
    "microservice",
    "nodejs",
    "express",
    "mongoose"
  ],
  "author": "ZodiaCore Team",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.2.0",
    "mongoose": "^7.5.0",
    "rate-limiter-flexible": "^3.0.4",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krnjsh87/ZodiaCore.git",
    "directory": "services/zc2-chinese"
  },
  "bugs": {
    "url": "https://github.com/krnjsh87/ZodiaCore/issues"
  },
  "homepage": "https://github.com/krnjsh87/ZodiaCore#readme"
}
